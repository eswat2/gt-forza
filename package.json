{
  "name": "gt-forza",
  "version": "0.0.282",
  "description": "prototype - simple serverless GraphQL api with mock data...",
  "author": "eswat2",
  "repository": "eswat2/gt-forza",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "got": "14.6.5",
    "graphql": "16.12.0",
    "graphql-http": "1.22.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "dev": "env-cmd -x -- vc dev --listen 8182 --token \\$API_TOKEN",
    "cc": "prettier --check './**/*.js'",
    "spell": "cspell './**/*.js'",
    "test": "echo skipping...",
    "format": "prettier --write server.js cSpell.json",
    "predeploy": "yarn version --patch",
    "deploy": "vc --prod",
    "open": "open https://gt-forza.vercel.app/graphql"
  },
  "devDependencies": {
    "env-cmd": "11.0.0",
    "prettier": "3.7.4",
    "vercel": "49.0.0"
  }
}
