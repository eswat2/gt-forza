{
  "name": "gt-forza",
  "version": "0.0.336",
  "description": "prototype - simple serverless GraphQL api with mock data...",
  "author": "eswat2",
  "repository": "eswat2/gt-forza",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "got": "14.6.6",
    "graphql": "16.12.0",
    "graphql-http": "1.22.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "bump": "bumpp patch --yes",
    "deploy-app": "vc --prod",
    "release": "pnpm bump && pnpm test && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "env-cmd -x -- vc dev --listen 8182 --token \\$API_TOKEN",
    "cc": "prettier --check './**/*.js'",
    "spell": "cspell './**/*.js'",
    "test": "echo skipping...",
    "format": "prettier --write server.js cSpell.json",
    "open": "open https://gt-forza.vercel.app/graphql"
  },
  "devDependencies": {
    "bumpp": "10.4.1",
    "env-cmd": "11.0.0",
    "only-allow": "1.2.2",
    "prettier": "3.8.1",
    "vercel": "50.22.1",
    "yoctocolors": "2.1.2"
  }
}
