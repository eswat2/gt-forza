{
  "name": "gt-forza",
  "version": "0.0.159",
  "description": "prototype - simple serverless GraphQL api with mock data...",
  "author": "eswat2",
  "repository": "eswat2/gt-forza",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "got": "14.4.2",
    "graphql": "16.9.0",
    "graphql-http": "1.22.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "dev": "vc dev",
    "cc": "prettier --check './**/*.js'",
    "spell": "cspell './**/*.js'",
    "test": "echo skipping...",
    "format": "prettier --write server.js cSpell.json",
    "predeploy": "yarn version --patch",
    "deploy": "vc --prod",
    "open": "open https://gt-forza.vercel.app/graphql"
  },
  "devDependencies": {
    "prettier": "3.3.3"
  }
}
