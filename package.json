{
  "name": "gt-forza",
  "version": "0.0.183",
  "description": "prototype - simple serverless GraphQL api with mock data...",
  "author": "eswat2",
  "repository": "eswat2/gt-forza",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "got": "14.4.7",
    "graphql": "16.11.0",
    "graphql-http": "1.22.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "dev": "env-cmd -x -- vc dev --listen 8182 --token \\$API_TOKEN",
    "cc": "prettier --check './**/*.js'",
    "spell": "cspell './**/*.js'",
    "test": "echo skipping...",
    "format": "prettier --write server.js cSpell.json",
    "predeploy": "yarn version --patch",
    "deploy": "vc --prod",
    "open": "open https://gt-forza.vercel.app/graphql"
  },
  "devDependencies": {
    "env-cmd": "10.1.0",
    "prettier": "3.5.3",
    "vercel": "41.7.4"
  }
}
